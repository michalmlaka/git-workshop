git init $PROJECT-PATH --init dir 

cd $PROJECT-PATH 

git add . -- add all changes in dir

git commit --commit changes

git status --show current status

git log --show changes

git remote add origin git@github.com:michalmlaka/$repo-name.git  --add github repo via ssh as origin

git push -u origin master --push

git pull -u origin master --pull and commit

git branch $name --create new branch

git checkout $name --change branch

git merge $branch-name --merge current branch  with $branch-name

test merge conflict

ahoj


  
